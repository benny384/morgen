public void entfernen(SchülerNachDenFerien s) {
    boolean entfernt = false;
    for (int i = 0; i < counter; i++) {
        if (liste[i].equals(s)) {
            for (int j = i; j < counter - 1; j++) {
                liste[j] = liste[j+1];
            }
            liste[counter-1] = null;
            counter--;
            entfernt = true;
            break;
        }
    }
    if (!entfernt) {
        System.out.println("Schüler nicht gefunden.");
    }
}

