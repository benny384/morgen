public class Dreieck {
    private double a, b, c; // Längen der drei Seiten
    
    // Konstruktor
    public Dreieck(double a, double b, double c) {
        this.a = a;
        this.b = b;
        this.c = c;
    }
    
    // Berechne den Umfang des Dreiecks
    public double umfang() {
        return a + b + c;
    }
    
    // Berechne den Flächeninhalt des Dreiecks
    public double flaecheninhalt() {
        double s = umfang() / 2;
        return Math.sqrt(s * (s - a) * (s - b) * (s - c));
    }
    
    // Berechne die Fläche des Dreiecks
    public double flaeche() {
        return 0.5 * a * b * Math.sin(Math.toRadians(winkelC()));
    }
    
    // Berechne den Winkel an der Seite c des Dreiecks
    public double winkelC() {
        return Math.toDegrees(Math.acos((a*a + b*b - c*c) / (2*a*b)));
    }
}
